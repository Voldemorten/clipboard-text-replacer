{
    "name": "localhost replacer",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "hello.html",
        "default_icon": "hello_extensions.png"
    },
    "permissions": ["contextMenus", "clipboardWrite", "activeTab", "scripting", "tabs"],
    "background": {
        "service_worker": "scripts/background.js"
    }
}
